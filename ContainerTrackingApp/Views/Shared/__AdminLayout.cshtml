<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="@Url.Content("~/css/bootstrap.css")" rel="stylesheet">
    <link href="@Url.Content("~/css/theme.css")" rel="stylesheet">
</head>
<body role="document">
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Container Tracking ", "", "", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">

                    <li>@Html.ActionLink("Company Details", "Index", "RegisterCompany", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Application Secret", "GetApplicationKey", "ApplicationKeys", new { area = "" }, null)</li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle.btn-success" data-toggle="dropdown">Tracking Information <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("List Of GeneratedKeys", "ListOfGeneratedKeys", "ApplicationKeys", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Request Information", "RequestContainer", "RequestInformation", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Request Container", "RequestContainer", "RequestInformation", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Container Tracking History", "RequestContainer", "RequestInformation", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Tracking Filter", "RequestContainer", "RequestInformation", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Exception Detail", "Index", "ExceptionDetails", new { area = "" }, null)</li>
                        </ul>
                    </li>
                    @*<li>@Html.ActionLink("Help", "Index", "Help", new { area = "" }, null)</li>*@
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        @Html.ActionLink("Logout", "Logout", "Login", new { area = "" }, null)
                    </li>
                </ul>
            </div>
            
        </div>
    </div>
    <div class="container-fluid">
        @*container body-content*@
        @RenderBody()
        <hr />
        @*<footer>
            <p>&copy; @DateTime.Now.Year</p>
        </footer>*@
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="@Url.Content("~/js/bootstrap.js")"></script>
</body>
</html>
